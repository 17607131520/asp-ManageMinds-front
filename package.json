{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --mode development",
    "start:production": "vite --mode production",
    "start:staging": "vite --mode staging",
    "start:test": "vite --mode test",
    "build:production": "tsc -b && vite build --mode production",
    "build:staging": "tsc -b && vite build --mode staging",
    "build:test": "tsc -b && vite build --mode test",
    "preview": "vite preview --mode test",
    "preview:test": "vite preview --mode test",
    "preview:staging": "vite preview --mode staging",
    "preview:production": "vite preview --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/charts": "^2.2.7",
    "@ant-design/icons": "^6.0.0",
    "@ant-design/pro-components": "^2.8.7",
    "@antv/g2": "^5.3.2",
    "@antv/g6": "^5.0.45",
    "@antv/l7": "^2.22.5",
    "@antv/l7-maps": "^2.22.5",
    "@antv/s2": "^2.3.1",
    "@antv/s2-react": "^2.2.0",
    "@antv/s2-react-components": "^2.1.2",
    "@antv/x6": "^2.18.1",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.15.2",
    "@uidotdev/usehooks": "^2.4.1",
    "@vitejs/plugin-legacy": "^6.1.0",
    "ahooks": "^3.8.4",
