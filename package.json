    "cesium": "^1.128.0",
    "dayjs": "^1.11.13",
    "dexie": "^4.0.11",
    "echarts": "^5.6.0",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^9.1.7",
    "js-cookie": "^3.0.5",
    "less": "^4.3.0",
    "lint-staged": "^15.5.1",
    "lodash": "^4.17.21",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-query": "^3.39.3",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.5.2",
    "react-virtualized": "^9.22.6",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^3.1.0",
